{
    "name": "smarteknoloji/blade-circle-flags",
    "description": "A package to easily make use of Circle Flags in your Laravel Blade views.",
    "keywords": ["Blade", "Circle Flags", "Laravel"],
    "homepage": "https://github.com/fahrim/blade-circle-flags",
    "license": "MIT",
    "authors": [
        {
            "name": "Fahri Meral",
            "email": "fahri.meral@smarteknoloji.com",
            "homepage": "https://www.fahrimeral.com.tr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-icons": "^1.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Smarteknoloji\\BladeCircleFlags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smarteknoloji\\BladeCircleFlags\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smarteknoloji\\BladeCircleFlags\\BladeCircleFlagsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
